rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // By default, allow read/write access only for authenticated users.
    // You should customize these rules to secure your files.
    // For example, you might want to restrict write access to specific paths or user roles.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
