rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // By default, allow read/write access only for authenticated users.
    // This is a starting point, and you should refine these rules
    // based on your application's specific security needs.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
